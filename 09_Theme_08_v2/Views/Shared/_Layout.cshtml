@{
    AnperoFrontend.WebService.Webconfig commonInfo = new AnperoFrontend.WebService.Webconfig();
    if (ViewData["commonInfo"] != null)
    {
        commonInfo = (AnperoFrontend.WebService.Webconfig)ViewData["commonInfo"];
    }
}
<!DOCTYPE html>
<html amp lang="vi" xmlns="http://www.w3.org/1999/xhtml" xmlns:fb="http://ogp.me/ns/fb#">
<head>
    <title>@ViewBag.Title</title>
  
    @Styles.Render("~/Content/css")
  
    <link rel="icon" href="@commonInfo.Logo" type="image/x-icon">
    <link rel="shortcut icon" href="@commonInfo.Logo" type="image/x-icon">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="Refresh" content="3600" />
    <meta name="viewport" content="width=device-width; initial-scale=1.0; maximum-scale=1.0; user-scalable=no">
    <meta name="robots" content="*">
    <!-- Apple iOS and Android stuff -->
    <meta name="apple-mobile-web-app-capable" content="no" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    @*AMP*@
    <script async src="https://cdn.ampproject.org/v0.js"></script>
    <style amp-boilerplate>
        body {
            -webkit-animation: -amp-start 8s steps(1,end) 0s 1 normal both;
            -moz-animation: -amp-start 8s steps(1,end) 0s 1 normal both;
            -ms-animation: -amp-start 8s steps(1,end) 0s 1 normal both;
            animation: -amp-start 8s steps(1,end) 0s 1 normal both
        }

        @@-webkit-keyframes -amp-start {
            from {
                visibility: hidden
            }

            to {
                visibility: visible
            }
        }

        @@-moz-keyframes -amp-start {
            from {
                visibility: hidden
            }

            to {
                visibility: visible
            }
        }

        @@-ms-keyframes -amp-start {
            from {
                visibility: hidden
            }

            to {
                visibility: visible
            }
        }

        @@-o-keyframes -amp-start {
            from {
                visibility: hidden
            }

            to {
                visibility: visible
            }
        }

        @@keyframes -amp-start {
            from {
                visibility: hidden
            }

            to {
                visibility: visible
            }
        }
    </style>
    <noscript>
        <style amp-boilerplate>
            body {
                -webkit-animation: none;
                -moz-animation: none;
                -ms-animation: none;
                animation: none
            }
        </style>
    </noscript>
    @*end AMP*@
   
    <link rel="stylesheet" type="text/css" href="/assets/stylesheet/responsive.css" media="all">
    <link rel="stylesheet" type="text/css" href="/assets/stylesheet/font-awesome.css" media="all">
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400italic,600italic,700italic,400,600,700,800' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Roboto+Slab:400,700' rel='stylesheet' type='text/css'>
    @RenderSection("health", required: false)
    <link rel="dns-prefetch" href="https://v2.zopim.com/">
    <link rel="dns-prefetch" href="https://www.google-analytics.com/">
    <link rel="dns-prefetch" href="http://fonts.googleapis.com/">
    <link rel="dns-prefetch" href="https://www.facebook.com/">
    <link href='/static/css' rel='stylesheet' type='text/css'>
  
   
</head>
<body>
    <h1 id="heading1" style="display:none;">
        @commonInfo.Desc
    </h1>
    @Html.Partial("_partial/_MainHeader_1")

    @RenderBody()
    <script type="text/javascript" src="/Scripts/jquery-1-10.js"></script>
    @Scripts.Render("~/bundles/scripts")

    @Html.Raw(commonInfo.AnperoPlugin)

    @Html.Raw(commonInfo.PageScript)

    @Html.Partial("_partial/_MainFooter_1")
  
    @RenderSection("scripts", required: false)   
    <script type="text/javascript">
        jQuery(window).load(function () {
            Cart.bindCart();
        });
    </script>
</body>
</html>