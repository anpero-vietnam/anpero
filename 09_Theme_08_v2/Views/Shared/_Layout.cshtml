@{
    AnperoFrontend.WebService.Webconfig commonInfo = new AnperoFrontend.WebService.Webconfig();
    if (ViewData["commonInfo"] != null)
    {
        commonInfo = (AnperoFrontend.WebService.Webconfig)ViewData["commonInfo"];
    }
    string baseUrl = Request.Url.Scheme + "://" + Request.Url.Authority + Request.ApplicationPath.TrimEnd('/');
}
<!DOCTYPE html>
<html amp lang="vi">
<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>

    @Styles.Render("~/Content/css")
    <link rel="dns-prefetch" href="https://v2.zopim.com/">
    <link rel="dns-prefetch" href="https://www.google-analytics.com/">
    <link rel="dns-prefetch" href="https://fonts.googleapis.com/">
    <link rel="dns-prefetch" href="https://www.facebook.com/">
    <link rel="icon" href="@commonInfo.Logo" type="image/x-icon">
    <link rel="shortcut icon" href="@(string.IsNullOrEmpty(commonInfo.Favicon)?commonInfo.Logo:commonInfo.Favicon)" type="image/x-icon">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="Refresh" content="3600" />
    <meta name="viewport" content="width=device-width,minimum-scale=1,initial-scale=1">
    <meta name="robots" content="*">
    <!-- Apple iOS and Android stuff -->
    <meta name="apple-mobile-web-app-capable" content="no" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <meta name="google-site-verification" content="MLrz3kB0o305th5xcLC59DsjVdKfictiXbflXiJRzsM" />

    <link rel="stylesheet" type="text/css" href="/assets/stylesheet/responsive.css" media="all">
    <link rel="stylesheet" type="text/css" href="/assets/stylesheet/font-awesome.css" media="all">
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400italic,600italic,700italic,400,600,700,800' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Roboto+Slab:400,700' rel='stylesheet' type='text/css'>
    @RenderSection("health", required: false)

    <link href='/static/css' rel='stylesheet' type='text/css'>
</head>
<body>
    <h1 id="heading1" style="display:none;">
        @commonInfo.Desc
    </h1>
    @Html.Partial("_partial/_MainHeader_1")

    @RenderBody()
    <script type="text/javascript" src="/Scripts/jquery-1-10.js"></script>
    @Scripts.Render("~/bundles/scripts")

    @Html.Raw(commonInfo.AnperoPlugin)

    @Html.Raw(commonInfo.PageScript)
    <!-- Histats.com  START  (aync)-->
    <script type="text/javascript">
        var _Hasync = _Hasync || [];
        _Hasync.push(['Histats.start', '1,4197650,4,0,0,0,00010000']);
        _Hasync.push(['Histats.fasi', '1']);
        _Hasync.push(['Histats.track_hits', '']);
        (function () {
            var hs = document.createElement('script'); hs.type = 'text/javascript'; hs.async = true;
            hs.src = ('//s10.histats.com/js15_as.js');
            (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(hs);
        })();</script>
    <noscript><a href="/" target="_blank"><img src="//sstatic1.histats.com/0.gif?4197650&101" alt="" border="0"></a></noscript>
    <!-- Histats.com  END  -->

    @Html.Partial("_partial/_MainFooter_1")

    @RenderSection("scripts", required: false)
    <script type="text/javascript">
        jQuery(window).load(function () {
            Cart.bindCart();
        });
    </script>
    <style>
        .zalo-chat-widget {
            bottom: 84px !important;
            right: 21px !important;
        }
    </style>    
    <script type="text/javascript" src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-5b7a515bc8da872d"></script>
    <script type="application/ld+json">
        {
        "@@context": "https://schema.org",
        "@@type": "WebSite",
        "url": "@Url.Content("~")",
        "potentialAction": {
        "@@type": "SearchAction",
        "target": "@(Url.Content("~").TrimEnd('/') + Url.RouteUrl("search") + "?keyword={keyword}")",
        "query-input": "required name=keyword"
        }
        }
    </script>
    <script type="application/ld+json">
        {
        "@@context": "https://schema.org",
        "@@type": "Organization",
        "url": "@Url.Content("~")",
        "logo": "@commonInfo.Logo"
        }

    </script>
</body>
</html>