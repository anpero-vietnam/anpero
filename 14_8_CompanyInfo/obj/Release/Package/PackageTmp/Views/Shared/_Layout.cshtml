@{
    AnperoFrontend.WebService.Webconfig commonInfo = new AnperoFrontend.WebService.Webconfig();
    if (ViewData["commonInfo"] != null)
    {
        commonInfo = (AnperoFrontend.WebService.Webconfig)ViewData["commonInfo"];
    }
}
<!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//VI" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html lang="vi" xmlns="http://www.w3.org/1999/xhtml"
      xmlns:fb="http://ogp.me/ns/fb#">
<head>
    <title>@ViewBag.Title</title>
    @RenderSection("health", required: false)
    @Styles.Render("~/Content/css")
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="Refresh" content="3600" />
    <meta name="viewport" content="width=device-width; initial-scale=1.0; maximum-scale=1.0; user-scalable=no">
    <meta name="robots" content="*">
    <!-- Apple iOS and Android stuff -->
    <meta name="apple-mobile-web-app-capable" content="no" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <!-- Apple iOS and Android stuff - don't remove! -->
    <link rel="icon" href="#" type="image/x-icon">
    <link rel="shortcut icon" href="@commonInfo.Logo" type="image/x-icon">
    <link rel="stylesheet" type="text/css" href="/assets/stylesheet/jquery.bxslider.css">
    <link rel="stylesheet" type="text/css" href="/assets/stylesheet/font-awesome.css">
    <link rel="stylesheet" type="text/css" href="/assets/stylesheet/style.css" media="all">
    <link rel="stylesheet" type="text/css" href="/assets/stylesheet/responsive.css" media="all">
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400italic,600italic,700italic,400,600,700,800' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Roboto+Slab:400,700' rel='stylesheet' type='text/css'>
    @RenderSection("canonical", required: false)
</head>
<body>
    <h1 id="heading1" style="display:none;">
        @commonInfo.Desc
    </h1>
    @Html.Partial("_partial/_MainHeader_1")

    @RenderBody()
    <script type="text/javascript" src="/Scripts/jquery-1-10.js"></script>
    <script src="https://www.google.com/recaptcha/api.js"></script>
    <script src="/Scripts/Bootrap.notify.js"></script>
    @Scripts.Render("~/bundles/scripts")

    @Html.Raw(commonInfo.AnperoPlugin)

    @Html.Raw(commonInfo.PageScript)

    <section class="contact-area section-padding section-relative" id="contact">
        <div class="section-bg"></div>
        <div class="container">
            <div class="row">
                <div class="col-xs-12 col-sm-8 col-sm-offset-2">
                    <div class="page-title">
                        <h2 style="border:none;">Đăng ký và liên hệ</h2>
                        <p>Hãy để lại thông tin liên hệ của bạn để nhận thông tin miễn phí từ chúng tôi !</p>
                        <p>Hotline :  @commonInfo.HotLine</p>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12 col-sm-8 col-sm-offset-2">
                    <input type="text" class="form-control" id="name" placeholder="Tên *" required="">
                    <input class="form-control" id="email" placeholder="Email">
                    <input type="text" class="form-control" id="phone" placeholder="Số điện thoại *" required="">
                    <input type="text" class="form-control" id="address" placeholder="Địa chỉ *" required="">
                    <textarea class="form-control" rows="6" id="message" placeholder="Để lại lời nhắn" required=""></textarea>
                    <div class="form-selector mbt15">
                        <div id="g-recaptcha"></div>
                    </div>
                    <div class="text-center mb-25">
                        <button class="battn" id="btn-send-contact">Gửi thông tin</button>
                    </div>

                </div>
            </div>
        </div>
    </section>
    @Html.Partial("_partial/_MainFooter_1")
    <script type="text/javascript" src="/assets/js/parallax.js"></script>
    <script type="text/javascript" src="/assets/js/common.js"></script>

    <script type="text/javascript" src="/assets/js/owl.carousel.min.js"></script>
    <script type="text/javascript" src="/assets/js/jquery.mobile-menu.min.js"></script>

    @RenderSection("scripts", required: false)
    <script>
        $(document).ready(function () {
            resizeVideo();
        });
        function resizeVideo() {
            var contentW = $('.content-video').width();
            var iframe = $('.content-video iframe');
            var iframeW = iframe.css('width', contentW - 20);
            iframe.css('width', iframeW);
            var iframeH = contentW / 16 * 9;
            iframe.css('height', iframeH);
        }
        var googleCatcha;
        $(window).load(function () {
            googleCatcha = grecaptcha.render('g-recaptcha', {
                'sitekey': googleCapchaSitekey,
                'theme': 'light',
                'hl': 'vi'
            });
        });

    </script>
    <script type="application/ld+json">
        {
        "@@context": "https://schema.org",
        "@@type": "WebSite",
        "url": "@Url.Content("~")",
        "potentialAction": {
        "@@type": "SearchAction",
        "target": "/search?keyword={keyword}")",
        "query-input": "required name=keyword"
        }
        }
    </script>
    <script type="application/ld+json">
        {
        "@@context": "https://schema.org",
        "@@type": "Organization",
        "url": "@Url.Content("~").TrimEnd('/')",
        "logo": "@commonInfo.Logo"
        }

    </script>
</body>
</html>